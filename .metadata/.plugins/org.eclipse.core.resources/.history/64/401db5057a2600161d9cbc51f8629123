  //CI2C1_Init(&CI2C1_TDeviceDataPtr);
  /* Write your code here */
  /* For example: for(;;) { } */
  LDD_TError error = CI2C1_MasterSendBlock(CI2C1_DeviceData, &buffer, 9, LDD_I2C_SEND_STOP);
  if (error == ERR_OK){
	  LED_DEBUG_PutVal(1);
  }
  else {
	  LED_DEBUG_PutVal(0);
  }
  /*lcd.init ();
  lcd.clear ();
